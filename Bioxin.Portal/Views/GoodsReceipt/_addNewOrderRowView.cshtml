@{
    int sl = Convert.ToInt32(TempData["trSl"]);
}
<tr id="tr_@sl">

    <td id="ts_@sl">@sl</td>
    <td>
        <input type="text" class="form-control" id="itemName_@sl" style="width: 90px"/>
    
    </td>
    <td>
        <select class="form-control select2" id="itemCode_@sl" style="width: 280px" onchange="GetItemDetails(@sl)"></select>
    </td>
    <td>
        <input type="text" class="form-control" id="uom_@sl" style="width: 60px" disabled="disabled" />
    </td>
    <td>

        <input type="number" class="form-control" id="quantity_@sl" oninput="CalculateLineTotal(@sl)" />
    </td>
    <td>
        @*<input type="number" class="form-control" id="batchSerialNo_@sl" disabled="disabled" style="width: 80px" />*@
    </td>
    <td>

        <input type="number" class="form-control" id="price_@sl" oninput="CalculateLineTotal(@sl)" />
    </td>
    <td>
        <input type="number" class="form-control" id="value_@sl" disabled="disabled" />
    </td>
    <td>
        <input type="text" class="form-control" id="remarks_@sl" />
    </td>
    @*<td class="actionHide">
        <div class="form-group">
            <button class="btn btn-xs btn-success " id="btnAdd_@sl" onclick="AddRow()"><b>+</b></button>
            <button class="btn btn-xs btn-danger" id="btnRemove_@sl" onclick="RemoveRow(@sl)"><b></b>x</button>
        </div>
    </td>*@
</tr>
