@{
    int sl = Convert.ToInt32(TempData["trSl"]);
}
<tr id="tr_@sl">

    <td id="ts_@sl">@sl</td>
    <td>
        @*<select class="form-control select2" id="pro_@sl" style="width: 150px"></select>*@
        <input type="text" class="form-control" id="pro_@sl" />
    </td>
    <td style="display:none">
        <input type="text" class="form-control" disabled="disabled" id="cat_@sl" />
    </td>
    <td>
        <input type="text" class="form-control" style="width: 350px" id="stQty_@sl" />
    </td>

    <td>
        <input type="number" class="form-control" id="stKg_@sl" />
    </td>
    <td>

        <input type="number" class="form-control" id="baseRate_@sl" />
    </td>
    <td>

        <input type="number" class="form-control" id="weightLoss_@sl" />
    </td>
    <td>
        <input type="text" class="form-control" id="stQty_@sl" />
    </td>
    <td>

        <input type="text" class="form-control" id="weightLoss_@sl" />
    </td>
    <td>

        <input type="number" class="form-control" id="weightLoss_@sl" />
    </td>
    <td class="actionHide">
        <div class="form-group">
            <button class="btn btn-xs btn-success " id="btnAdd_@sl" onclick="AddRow()"><b>+</b></button>
            <button class="btn btn-xs btn-danger" id="btnRemove_@sl" onclick="RemoveRow(@sl)"><b></b>x</button>
        </div>
    </td>
    @*<td class="aHide">
            <div class="form-group">
                <input type="checkbox" id="already_@sl" />
            </div>
        </td>*@




</tr>
