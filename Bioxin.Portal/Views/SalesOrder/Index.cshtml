<section class="content">
    <div class="container-fluid">
        <div class="row">
            <!-- left column -->
            <div class="col-md-12">
                <!-- general form elements -->
                <div class="card card-primary">

                    <div class="card-header">

                        <div class="row">

                            <div class="col-md-6">
                                <h3>Sales Order List Page</h3>
                            </div>
                            <div class="col-md-6 text-right ">
                                <a class="btn btn-outline-warning" href="@Url.Action("SalesOrderEntry", "SalesOrder", new {id = 0})">

                                    <span class="fa fa-plus"></span> Go To New Sales Order Entry

                                </a>
                            </div>

                        </div>



                    </div>
                    <div class="row">
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md-3">
                                    <div class="form-group" id="divCustomer_Search">
                                        <label for="ddlCustomer_Search">Customer</label>
                                        <select class="form-control select2" style="width: 100%;" id="ddlCustomer_Search"></select>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group" id="divSalesPerson">
                                        <label for="txtSalesPerson">Sales Person</label>
                                        <select class="form-control select2" style="width: 100%;" id="ddlSalesEmployee"></select>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group" id="divFromDate">
                                        <label for="txtFromDate">From Order Date</label>
                                        <input type="text" id="txtFromDate" class="form-control" />
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group" id="divToDate">
                                        <label for="txtToDate">To Order Date</label>
                                        <input type="text" id="txtToDate" class="form-control" />
                                    </div>
                                </div>
                            </div>

                        </div>

                    </div>
                    <div class="row">
                        <div class="col-md-3"></div>
                        <div class="col-md-3"></div>
                        <div class="col-md-3"></div>
                        <div class="col-md-3">
                            <button style="margin-right: 27px;" class="btn btn-primary" onclick="ShowReport()">Search Order</button>
                        </div>
                    </div>
                    <!-- /.card-header -->
                    <!-- form start -->
                    <div class="row">
                        <div class="col-md-12">
                            <div class="card-body">
                                <table class="table table-bordered table-striped table-hover">
                                    <thead>
                                        <tr style="background-color: aliceblue">
                                            <th>#</th>
                                            <th>Doc No</th>
                                            <th>Customer</th>
                                            <th>Contact Person</th>
                                            <th>Currency</th>
                                            <th>Posting Date</th>
                                            <th>Due Date</th>
                                            <th>Total</th>
                                            <th>Action</th>

                                        </tr>
                                    </thead>
                                    <tbody id="manuallist">
                                        @{ int count = 1;}
                                        @foreach (var item in Model)
                                        {
                                        <tr>
                                            <td>@count</td>
                                            <td>@item.DocNum</td>
                                            <td>@item.CardName</td>
                                            <td>@item.ContactPersonName</td>
                                            <td>@item.DocCurrency</td>
                                            <td>@Convert.ToDateTime(item.DocDate).ToString("dd-MMM-yyyy")</td>
                                            <td>@Convert.ToDateTime(item.DocDueDate).ToString("dd-MMM-yyyy")</td>
                                            <td>@item.DocTotal.ToString("##,###,0.00")</td>

