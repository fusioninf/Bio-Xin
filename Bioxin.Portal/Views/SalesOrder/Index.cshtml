        function ShowReport() {
            debugger;
            var fromdate;
            var todate;
            var customerid = '';
            var salespersonId = '0';
            if ($('#txtFromDate').val() == "") {
                swal("Please input from order date");
                return;
            }
            else {
                fromdate = $('#txtFromDate').val();
            }
            if ($('#txtToDate').val() == "") {
                swal("Please input to order date");
                return;
            }
            else {
                todate = $('#txtToDate').val();
            }
            if ($('#ddlSalesEmployee').val() == "0") {
                salespersonId = '0';
