            else {
                customerid = $('#ddlCustomer_Search').val();
            }

            var urlpath = '@Url.Action("SalesOrderList_Search", "SalesOrder")';
            $.ajax({
                url: urlpath,
                dataType: 'html',
                type: "Get",
                data: { fromdate: fromdate, todate: todate, customerid: customerid, salespersonId: salespersonId},
                async: false,
                success: function (data) {

                    data = JSON.parse(data);
                    if (data.length > 0) {
                        var row = "";
                        var count = 1;
                        $("#manuallist").html("");

                        var count=1;
                        $.each(data, function (index, item) {

                            row += "<tr>";
                            row += "<td style='text-align:center'>" + count + "</td>";
                            row += "<td style='text-align:center'>" + item.DocNum + "</td>";
                            row += "<td style='text-align:center'>" + item.CardName +  "</td>";
                            row += "<td style='text-align:center'>" + item.ContactPersonName +  "</td>";
                            row += "<td style='text-align:center'>" + item.DocCurrency + "</td>";
                            row += "<td style='text-align:center'>" + ToJavaScriptDate(item.DocDate) + "</td>";
                            row += "<td style='text-align:center'>" + ToJavaScriptDate(item.DocDueDate) + "</td>";
                            row += "<td style='text-align:center'>" + item.DocTotal + "</td>";

                            if (item.DocStatus == "O")
