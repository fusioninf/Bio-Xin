@{
    int sl = Convert.ToInt32(TempData["trSl"]);
}
<tr id="tr_@sl">

    <td id="ts_@sl">@sl</td>
    <td>
        <select class="form-control select2" id="Item_@sl" style="width: 150px" onchange="GetItemDetails(@sl)"></select>
        @*<input type="text" class="form-control" id="pro_@sl" />*@
    </td>
    <td>
        <input type="text" class="form-control" style="width: 350px" id="description_@sl" disabled/>
    </td>

    <td>
        <input type="number" class="form-control" id="quantity_@sl" oninput="CalculateLineTotal(@sl)"/>
    </td>
    <td>

        <input type="number" class="form-control" id="price_@sl" oninput="CalculateLineTotal(@sl)"/>
    </td>
    <td>

        <input type="number" class="form-control" id="discount_@sl" oninput="CalculateLineTotal(@sl)"/>
    </td>
    <td>
        <select class="form-control select2" id="taxWshCode_@sl" style="width: 150px"></select>
    </td>
    <td>

        <input type="number" class="form-control" id="total_@sl" disabled/>
    </td>
    <td>

        <input type="text" class="form-control" id="uom_@sl" disabled/>
    </td>
    <td>

        <input type="number" class="form-control" id="inStock_@sl" disabled/>
    </td>
    <td class="actionHide">
        <div class="form-group">
            <button class="btn btn-xs btn-success " id="btnAdd_@sl" onclick="AddRow()"><b>+</b></button>
            <button class="btn btn-xs btn-danger" id="btnRemove_@sl" onclick="RemoveRow(@sl)"><b></b>x</button>
            <input type="number" value="0" style="display: none" id="details_@sl" />
            <input type="number" value="-1" style="display: none" id="lineNum_@sl" />
        </div>
    </td>
    @*<td class="aHide">
            <div class="form-group">
                <input type="checkbox" id="already_@sl" />
            </div>
        </td>*@




</tr>
