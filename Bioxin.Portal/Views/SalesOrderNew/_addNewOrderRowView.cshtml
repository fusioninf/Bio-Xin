@{
    int sl = Convert.ToInt32(TempData["trSl"]);
}
<tr id="tr_@sl">

    <td id="ts_@sl">@sl</td>

    <td>
        @*<select class="form-control select2" id="itemCode_@sl" style="width: 150px" onchange="GetItemDetails(@sl)" disabled="disabled"></select>*@
        <input type="text" class="form-control" id="itemCode_@sl" style="width: 150px" disabled="disabled" />
    </td>
    <td>
        <input type="text" class="form-control" id="itemName_@sl" style="width: 250px" disabled="disabled" />
    </td>
    <td>
        <input type="number" class="form-control" id="session_@sl" style="width: 50px" disabled="disabled" />
    </td>
    <td>
        <input type="text" class="form-control" id="deliverydate_@sl" style="width: 120px" disabled="disabled" />
    </td>
    @*<td>
            <select class="form-control select2" id="fromWH_@sl" style="width: 150px"></select>
        </td>
        <td>

            <select class="form-control select2" id="toWH_@sl" style="width: 150px"></select>
        </td>*@
    <td>

        <input type="number" class="form-control" id="quantity_@sl" style="width: 80px" oninput="LineCalculation(@sl)" disabled="disabled" />
    </td>
    <td>
        <input type="text" class="form-control" id="uom_@sl" style="width: 80px" disabled="disabled" />
    </td>
    @*<td>
            <input type="number" class="form-control" id="batchSerialNo_@sl" />
        </td>*@
    <td>
        <input type="number" class="form-control" id="price_@sl" style="width: 150px" oninput="LineCalculation(@sl)" disabled="disabled" />
    </td>
    <td>
        <input type="number" class="form-control" id="discount_@sl" style="width: 80px" oninput="LineCalculation(@sl)" disabled="disabled" />
    </td>
    <td>
        <input type="number" class="form-control" id="discountamount_@sl" style="width: 150px" disabled="disabled" />
    </td>
    <td>
        <input type="text" class="form-control" id="taxcode_@sl" style="width: 100px" disabled="disabled" />
        <input type="hidden" id="taxrate_@sl" />
    </td>
    <td>
        <input type="number" class="form-control" id="taxamount_@sl" style="width: 150px" disabled="disabled" />
    </td>

    <td>
        <input type="number" class="form-control" id="total_@sl" style="width: 150px" disabled="disabled" />
        <input type="hidden" id="totalbeforetax_@sl" />
    </td>
    <td>
        <input type="number" class="form-control" id="stock_@sl" style="width: 100px" disabled="disabled" />
        @*<input type="hidden" class="form-control" id="deliverydate_@sl" style="width: 120px"  />*@
    </td>


    @*<td class="actionHide">
            <div class="form-group">
                <button class="btn btn-xs btn-success " id="btnAdd_@sl" onclick="AddRow()"><b>+</b></button>
                <button class="btn btn-xs btn-danger" id="btnRemove_@sl" onclick="RemoveRow(@sl)"><b></b>x</button>
            </div>
        </td>*@
</tr>
