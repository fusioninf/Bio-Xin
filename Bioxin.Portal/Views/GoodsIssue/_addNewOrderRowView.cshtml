@{
    int sl = Convert.ToInt32(TempData["trSl"]);
}
<tr id="tr_@sl">

    <td id="ts_@sl">@sl</td>
    <td>
        <input type="text" class="form-control" id="itemCode_@sl" style="width: 90px" disabled="disabled"/>
       
    </td>
    <td>
        <input type="text" class="form-control" id="itemName_@sl" style="width: 280px" disabled="disabled" />
        @*<select class="form-control select2" id="itemCode_@sl" style="width: 280px" onchange="GetItemDetails(@sl)" disabled></select>*@
    </td>
    <td>
        <input type="text" class="form-control" id="uom_@sl" style="width: 60px" disabled="disabled" />
    </td>
    <td>

        <input type="number" class="form-control" id="quantity_@sl" oninput="CalculateLineTotal(@sl)" disabled="disabled"/>
    </td>
    <td>
        <button class="btn btn-sm btn-primary showBtn" data-bs-toggle="modal" data-bs-target="#viewModal" onclick="ShowData(@sl)" id="showBtn_@sl" style="display:none">Batches</button>
        <button class="btn btn-sm btn-primary showBtn" data-bs-toggle="modal" data-bs-target="#viewModal" onclick="ShowSerialData(@sl)" id="showSerialBtn_@sl" style="display:none">Serial</button>
        @*<input type="number" class="form-control" id="batchSerialNo_@sl" disabled="disabled" style="width: 80px" />*@
    </td>
    <td>

        <input type="text" class="form-control" id="deptCost_@sl" style="width: 120px" disabled="disabled" />
    </td>
    <td>
        <input type="text" class="form-control" id="empCost_@sl" style="width: 120px" disabled="disabled" />
    </td>
    <td>
        <input type="hidden" class="form-control" id="price_@sl" oninput="CalculateLineTotal(@sl)" />
        <input type="hidden" class="form-control" id="value_@sl" disabled="disabled" />
        <input type="text" class="form-control" id="remarks_@sl" disabled="disabled"/>
    </td>
    @*<td class="actionHide">
        <div class="form-group">
            <button class="btn btn-xs btn-success " id="btnAdd_@sl" onclick="AddRow()"><b>+</b></button>
            <button class="btn btn-xs btn-danger" id="btnRemove_@sl" onclick="RemoveRow(@sl)"><b></b>x</button>
        </div>
    </td>*@
</tr>
